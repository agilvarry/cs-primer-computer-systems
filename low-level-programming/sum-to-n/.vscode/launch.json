{
  "configurations": [
    {
      "type": "gdb",
      "request": "launch",
      "name": "GDB64",
      "target": "${fileDirname}/${fileBasenameNoExtension}",
      "cwd": "${workspaceRoot}",
      "valuesFormatting": "parseText",
      "preLaunchTask": "asm64",
      "autorun": [
        "set output-radix 16"
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/agilvarry/github/cs-primer-computer-systems/low-level-programming/sum-to-n",
      "program": "/home/agilvarry/github/cs-primer-computer-systems/low-level-programming/sum-to-n/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
