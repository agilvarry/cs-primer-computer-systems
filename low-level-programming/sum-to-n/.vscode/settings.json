{
  "debug.allowBreakpointsEverywhere": true,
  "filewatcher.commands": [
    {
      "match": "\\.c",
      "autoClearConsole": true,
      "isClearStatusBar": true,
      "cmd": "cd ${workspaceRoot} \n cc ${file} \n ${workspaceRoot}/a.out",
      "event": "onFileChange"
    }
  ],
  "C_Cpp_Runner.msvcBatchPath": ""
}